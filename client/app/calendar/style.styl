.kc-calendar
  position relative
  width 633px
  height 610px

  .kc-times
    position absolute
    top 30px

  .kc-calendar-content
    margin-left 60px
    position relative
    height 100%

  .kc-days>div
    display inline-block
    padding 0 10px

  .kc-event
    position absolute
    background-color #b2b2f8
    border-radius 5px
    padding 5px
    font-size 10px
